//parcial 1 2024
public class ModeloP1 {

public ModeloP1 () {

}
/*
 * hacer un progrma en java que te de veulva c
 * cuantos valores estan por ensima del promedio 
 * 
 * 
 */

public int ensimaDelPromedio (int [] valores) {
 double suma = 0;
 for(int i =0; i < valores.length; i ++){
    suma += valores[i];
 }

double promedio = (suma / valores.length);

int contar = 0;
for(int i =0; i < valores.length; i ++){
   if(valores[i] > promedio)
         contar++;
}
return contar;
}




public static void main(String[] args) {
    ModeloP1 model = new ModeloP1();
    int [] valores = {1,3,4,2,4,-9,0,4};
    System.out.println(model.ensimaDelPromedio(valores));
}



}
